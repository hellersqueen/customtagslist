// Function to check and add linked tags
const checkAndAddLinkedTags = async () => {
  // Check if the "selected-tags" div exists
  const selectedTagsElement = document.getElementById('selected-tags');
  if (!selectedTagsElement) {
    return;
  }

  // Get all the tags within the "selected-tags" div
  const tags = Array.from(selectedTagsElement.querySelectorAll('.unn5w'));

    // Loop through each tag and check if it exists in linkedTags
  tags.forEach(async (tagElement) => {
    const tagName = tagElement.textContent.trim();

    if (linkedTags.hasOwnProperty(tagName)) {
      const linkedTagsToAdd = linkedTags[tagName];

      // Call the editPostFormTags function to add the linked tags
      await editPostFormTags({ add: linkedTagsToAdd, remove: [] });

      // Log a message when tags are added
      console.log(`Added linked tags for tag: ${tagName}`);
    }
  });
};

// Create a MutationObserver to watch for changes in the page
const observer = new MutationObserver(() => {
  // Call the checkAndAddLinkedTags function whenever a change is detected
  checkAndAddLinkedTags();
});

// Configure the observer to watch for changes in the body of the page
const observerConfig = { childList: true, subtree: true };

// Start observing the page for changes
observer.observe(document.body, observerConfig);

// Call the checkAndAddLinkedTags function initially to handle the current state of the page
checkAndAddLinkedTags();
